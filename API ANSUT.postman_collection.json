  "event": [
    {
      "listen": "prerequest",
      "script": {
        "type": "text/javascript",
        "requests": {},
        "exec": [
          "pm.sendRequest({",
          "    url: pm.environment.get(\"GATEWAY_URL\"),",
          "    method: 'POST',",
          "    header: {",
          "        'Content-Type': 'application/json' ",
          "    },",
          "    body: {",
          "        mode: 'raw',",
          "        raw: JSON.stringify({ apiKey: pm.environment.get(\"API_KEY\"), secretKey: pm.environment.get(\"SECRET_KEY\") })",
          "    }",
          "}, function (err, response) {",
          "    pm.environment.set(\"BEARER_TOKEN\", response.json().bearerToken);",
          "    console.log(response.json());",
          "});"
        ]
      }
