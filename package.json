{
  "name": "mon-toit-platform",
  "private": true,
  "version": "3.2.2",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --config vite.config.optimized.ts",
    "build:dev": "vite build",
    "build:standard": "vite build",
    "build:analyze": "vite build --config vite.config.optimized.ts && open stats.html",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:memory": "vitest run tests/memory-leaks-validation.test.ts",
    "test:memory:watch": "vitest tests/memory-leaks-validation.test.ts --watch",
    "memory-check": "node check-memory-leaks.cjs",
    "memory-check:file": "node check-memory-leaks.cjs --file",
    "memory-audit": "npm run memory-check && npm run test:memory",
    "prepare": "husky install",
    "test:security": "vitest --config vitest.security.config.ts",
    "test:security:watch": "vitest --config vitest.security.config.ts --watch",
    "test:security:coverage": "vitest --config vitest.security.config.ts --coverage",
    "test:security:ui": "vitest --config vitest.security.config.ts --ui",
    "test:security:run": "vitest --config vitest.security.config.ts --run",
    "test:security:report": "vitest --config vitest.security.config.ts --reporter=html --outputFile=security-report.html",
    "test:integration:security": "vitest --config vitest.security.config.ts src/tests/security.integration.test.ts",
    "test:penetration": "node src/tests/penetration-test.cjs",
    "test:security:all": "npm run test:security:run && npm run test:integration:security && npm run test:penetration"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@capacitor/android": "^7.4.4",
    "@capacitor/app": "^7.1.0",
    "@capacitor/camera": "^7.0.2",
    "@capacitor/cli": "^8.0.1",
    "@capacitor/core": "^7.4.4",
    "@capacitor/geolocation": "^7.1.6",
    "@capacitor/haptics": "^7.0.2",
    "@capacitor/ios": "^7.4.4",
    "@capacitor/keyboard": "^7.0.3",
    "@capacitor/preferences": "^7.0.2",
    "@capacitor/push-notifications": "^7.0.3",
    "@capacitor/share": "^7.0.2",
    "@capacitor/splash-screen": "^7.0.3",
    "@capacitor/status-bar": "^7.0.3",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-google-maps/api": "^2.20.7",
    "@sentry/react": "^10.26.0",
    "@sentry/vite-plugin": "^4.6.1",
    "@supabase/supabase-js": "^2.86.0",
    "@tanstack/react-query": "^5.90.5",
    "@types/mapbox-gl": "^3.4.1",
    "canvas-confetti": "^1.9.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "jspdf": "^4.0.0",
    "jspdf-autotable": "^5.0.7",
    "leaflet": "^1.9.4",
    "lovable-tagger": "^1.1.11",
    "lucide-react": "^0.344.0",
    "mapbox-gl": "^3.16.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-ga4": "^2.1.0",
    "react-router-dom": "^7.12.0",
    "recharts": "^3.7.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tesseract.js": "^5.1.1",
    "xlsx": "^0.18.5",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/leaflet": "^1.9.21",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/ui": "^4.0.17",
    "autoprefixer": "^10.4.18",
    "browser-image-compression": "^2.0.2",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "husky": "^8.0.3",
    "idb": "^8.0.3",
    "jsdom": "^23.2.0",
    "lint-staged": "^15.5.2",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.35",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.5",
    "supabase": "^2.72.8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.17",
    "workbox-window": "^7.4.0"
  }
}
